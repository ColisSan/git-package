{
    "name": "colissan/git-package",
    "description": "Backend for nova abi reader tool",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "zarko13",
            "email": "zarsarenac@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GitPackage\\": "src/",
            "App\\" : "src/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GitPackage\\GitPackageServiceProvider"
            ]
        }
    }
}